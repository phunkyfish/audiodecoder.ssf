msgstr "Salida directa"

msgctxt "#30016"
msgid "Enable direct (dry) output."
msgstr "Activar la salida directa (en seco)."

msgctxt "#30017"
msgid "DSP emulation"
msgstr "Emulación DSP"

msgctxt "#30018"
msgid "Enable DSP emulation (for reverb, etc.)."
msgstr "Activar la emulación DSP (para reverberaciones, etc.)."

msgctxt "#30019"
msgid "Dynamic recompiler"
msgstr "Recompilador dinámico"

msgctxt "#30020"
msgid "Emulate DSP using dynamic recompiler."
msgstr "Emular el DSP utilizando el recompilador dinámico."
