msgctxt "#30000"
msgid "Default length"
msgstr "Zadana duljina"

msgctxt "#30001"
msgid "Used if specified file does not provide the length."
msgstr "Korist se ako određena datoteka nema zadanu duljinu."

msgctxt "#30002"
msgid "unused"
msgstr "nekorišteno"

msgctxt "#30003"
msgid "{0:d} s"
msgstr "{0:d} s"

msgctxt "#30004"
msgid "Default fade out time"
msgstr "Zadano vrijeme izbljeđivanja"

msgctxt "#30005"
msgid "Used if specified file does not provide the length and fade."
msgstr "Korist se ako određena datoteka nema zadanu duljinu i izbljeđivanje."

msgctxt "#30006"
msgid "unused"
msgstr "nekorišteno"

msgctxt "#30007"
msgid "{0:d} ms"
msgstr "{0:d} ms"

msgctxt "#30008"
msgid "Suppress opening silence"
msgstr "Potisni uvodnu tišinu"

msgctxt "#30009"
msgid "Some files start with silent data, this prevents it and looks for the beginning."
msgstr "Određene datoteke počinju s tišinom, to ju sprječava i odmah započinje."

msgctxt "#30010"
msgid "Suppress end silence"
msgstr "Potisni završnu tišinu"

msgctxt "#30011"
msgid "Some files end with silent data, this prevents it and looks for the end."
msgstr "Određene datoteke završavaju s tišinom, to ju sprječava i odmah završava."

msgctxt "#30012"
msgid "Second of silence to check"
msgstr "Sekundi tišine za provjeru"

msgctxt "#30013"
msgid "How many silent seconds are allowed before playback is stopped."
msgstr "Koliko sekundi tišine je dopušteno prije nego što se reprodukcija zaustavi."

msgctxt "#30014"
msgid "{0:d} s"
msgstr "{0:d} s"

msgctxt "#30015"
msgid "Direct output"
msgstr "Izravan izlaz"

msgctxt "#30016"
msgid "Enable direct (dry) output."
msgstr "Omogući izravan (dry) izlaz."

msgctxt "#30017"
msgid "DSP emulation"
msgstr "DSP emulacija"

msgctxt "#30018"
msgid "Enable DSP emulation (for reverb, etc.)."
msgstr "Omogući DSP emulaciju (za odjek, itd.)."

msgctxt "#30019"
msgid "Dynamic recompiler"
msgstr "Dinamički rekompilator"

msgctxt "#30020"
msgid "Emulate DSP using dynamic recompiler."
msgstr "Emuliraj DSP koristeči dinamički rekompilator."
